                        android:layout_height="wrap_content"
                        android:background="@color/colorBlack"
                        android:orientation="horizontal">

                        <TextView
                            android:text="Otros:"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_margin="1dp"
                            android:layout_weight="0.405"
                            android:background="@color/white"
                            android:gravity="center_vertical"
                            android:paddingStart="5dp"
                            android:textColor="@color/color_logo"
                            android:textSize="15sp"/>


                        <EditText
                            android:id="@+id/edOtros"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_margin="1dp"
                            android:layout_weight="0.61"
                            android:hint="---"
                            android:paddingStart="5dp"
                            android:textColor="@color/color_logo"
                            android:gravity="start"
                            android:background="@color/white"
                            android:inputType="text"
                            android:singleLine="true" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/colorBlack"
                        android:orientation="horizontal">

                        <TextView
                            android:text="Observaciones:"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_margin="1dp"
                            android:layout_weight="0.405"
                            android:background="@color/white"
                            android:paddingStart="5dp"
                            android:textColor="@color/color_logo"
                            android:textSize="15sp"/>


                        <EditText
                            android:id="@+id/edObservacion"
                            android:layout_width="0dp"
                            android:layout_height="60dp"
                            android:layout_margin="1dp"
                            android:layout_weight="0.61"
                            android:hint="---"
                            android:paddingStart="5dp"
                            android:textColor="@color/color_logo"
                            android:gravity="start"
                            android:background="@color/white"
                            android:inputType="text"
                            android:singleLine="true" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/colorBlack"
                        android:orientation="horizontal">

                        <TextView
                            android:text="Firma del Tecnico Responsable:"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_margin="1dp"
                            android:layout_weight="0.405"
                            android:background="@color/white"
                            android:paddingStart="5dp"
                            android:textColor="@color/color_logo"
                            android:textSize="15sp"/>


                        <ImageView
                            android:id="@+id/btFirma"
                            android:layout_width="0dp"
                            android:layout_height="80dp"
                            android:layout_margin="1dp"
                            android:layout_weight="0.61"
                            android:paddingStart="5dp"
                            android:gravity="start"
                            android:background="@color/white" />

                    </LinearLayout>
                </LinearLayout>

            </ScrollView>

        </LinearLayout>


    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="0.15"
        android:gravity="bottom"
        android:orientation="horizontal">

        <Button
            android:id="@+id/btInsumoEnviar"
            android:layout_width="300dp"
            android:layout_height="60dp"
            android:layout_marginStart="30dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="5dp"
            android:background="@drawable/button_selector"
            android:text="ENVIAR"
            android:textColor="@color/white"
            android:textSize="20sp" />

        <Button
            android:id="@+id/btInsumoRegresa"
            android:layout_width="300dp"
            android:layout_height="60dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="5dp"
            android:background="@drawable/button_selector"
            android:text="REGRESAR"
            android:textColor="@color/white"
            android:textSize="20sp" />


    </LinearLayout>

</LinearLayout>